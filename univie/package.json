{
  "name": "plugin-univie",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "pluginMetadata": {
    "id": "plugin-univie",
    "name": "University of Vienna",
    "description": "Univie plugin: config, sidebar, footer, landing page, empty state, event calendar.",
    "category": "feature",
    "icon": "School",
    "workspaceDependencies": {
      "@workspace/plugin-system": ">=1.0.0"
    }
  },
  "main": "./dist/univie.mjs",
  "exports": {
    ".": "./dist/univie.mjs"
  },
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf .turbo && rm -rf dist && rm -rf dist-types && rm -rf target && rm -rf node_modules && rm -rf coverage && rm -f *.tsbuildinfo && rm -f tsconfig*.tsbuildinfo",
    "build": "vite build",
    "dev:calendar": "pnpm --filter @univie/event-calendar dev",
    "build:calendar": "pnpm --filter @univie/event-calendar build",
    "preview:calendar": "pnpm --filter @univie/event-calendar preview",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@workspace/app-runtime": "workspace:*",
    "@workspace/i18n": "workspace:*",
    "@workspace/plugin-system": "workspace:*",
    "@workspace/query": "workspace:*",
    "@workspace/router": "workspace:*",
    "@workspace/ui": "workspace:*",
    "@workspace/utils": "workspace:*",
    "lucide-react": "^0.417.0",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.6",
    "@types/react": "^18.0.0 || ^19.0.0",
    "@types/react-dom": "^18.0.0 || ^19.0.0",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "@workspace/vite-config": "workspace:*",
    "eslint": "^9.9.0",
    "typescript": "~5.5.4",
    "vite": "^6.3.5"
  }
}
